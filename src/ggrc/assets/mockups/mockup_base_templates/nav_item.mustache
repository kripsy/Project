{{#each views}}
<li {{#active}}class="active"{{/active}}>
    <a href="#{{title}}">
        <i class="fa fa-{{icon}} color"></i>
        {{title}} {{#if children}}({{children.length}}){{/if}}
    </a>
</li>
{{/each}}
