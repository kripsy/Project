{{^list}}
  <div class="rtf" style="margin:20px">
    <p class="no-data">
      No objectives linked to this section
    </p>
  </div>
{{/list}}
{{#if list.length}}
<ul class='tree-structure simple'>
  {{#list}}
  <li class='objective'>
    <div class='item-main'>
      <div class='item-title'>
        <div class='row-fluid'>
          <div class='span12'>
            <div class='oneline'>
              <a href='{{viewLink}}'>
                <span class="title">{{title}}</span>
                <span class="text-compliance">{{slug}}</span>
              </a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </li>
  {{/list}}
</ul>
{{/if}}
