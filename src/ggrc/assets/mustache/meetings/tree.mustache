<li class="tree-item {{#if instance.snapshot}}snapshot{{/if}}" data-object-id="{{instance.id}}" data-object-type="{{instance.class.table_singular}}">
    <div class="item-main" {{#instance}}{{data 'model'}}{{/instance}}>
      <div class="item-wrap">
        <div class="select">
          <div class="item-data">
            <div class="row-fluid">
              <div class="span12">
                <div class="title tree-title-area" title="{{instance.title}}">
                  <i class="fa fa-calendar"></i>
                  <a class="url" href="{{schemed_url instance.title}}" target="_blank">
                    {{instance.title}}
                  </a>
                  {{{!render_hooks 'Meeting.tree_view_info'}}}
                </div>
              </div>
            </div>
          </div><!-- item-data end -->
        </div><!-- select end -->
      </div><!-- item-wrap end -->
    </div><!-- item-main end -->
  </li>
