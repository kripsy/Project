<div class="selector-info">
  <h4>
    <i class="fa fa-link"></i>
    {{firstnonempty title link }}
  </h4>
  {{#if description}}
    <div class="rtf">
      {{{description}}}
    </div>
  {{/if}}
  <h4 class="simple-url">
    <a href="{{ link }}" target="_blank">
      {{ link }}
    </a>
  </h4>
</div>
