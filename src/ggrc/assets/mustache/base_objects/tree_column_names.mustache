<div class="span{{display_options.selectable_width}}">
  <div class="row-fluid">
    {{#display_attr_list}}
      <div class="span{{display_options.selectable_attr_width}}">
        <div class="oneline">
          <span class="widget-col-title" data-field="{{attr_sort_field}}">
            {{attr_title}}
            <i class="fa fa-caret-up"></i>
          </span>
        </div>
      </div>
    {{/display_attr_list}}
  </div>
</div>
