<ul class="nav nav-tabs">
  {{#each panels}}
  <li {{#if panel.active}}class="active"{{/if}}>
    <a {{data 'panel'}}
      can-click='setActive'
      data-index="{{@index}}"
      href="#tab-{{@index}}">
      {{title}}
    </a>
  </li>
  {{/each}}
</ul>
<content></content>
