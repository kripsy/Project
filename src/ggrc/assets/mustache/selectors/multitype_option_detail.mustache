<div class="selector-info" style="margin-left:-10%;">
  <h4>
    <i class="fa fa-{{class.table_singular}} color"></i>
    {{ title }} {{class.title_singular}}
    <span class="code-info">{{slug}}</span>
  </h4>
  {{#if description}}
    <div class="rtf">
      {{{description}}}
    </div>
  {{/if}}
  <div class="owner">
    {{^if link}}
      {{{render '/static/mustache/base_objects/contacts.mustache' instance=this stacked=true}}}
    {{/if}}
  </div>
  <div class="object-url">
    <strong>URL:</strong>
    {{#if url}}
    <a href="{{schemed_url url}}" target="_blank">{{url}}</a>
    {{/if}}
    {{#if link}}
    <a href="{{link}}" target="_blank">{{link}}</a>
    {{/link}}
  </div>
</div>
